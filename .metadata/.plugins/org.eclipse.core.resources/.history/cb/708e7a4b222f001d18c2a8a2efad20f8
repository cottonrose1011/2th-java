package gugudan;

public class Gugudan {
	//field
	private int dan = 5;
	private String name = "";
	
	//methods
	public Gugudan(int dan) {
		super();
		this.dan = dan;

		
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Gugudan() {
		super();
	}
	
	public void go() {
		
		for(int i=1; i<10; i++) {
			System.out.println(dan+" x "+i+" = "+dan*i);
		}
	}

	public int getDan() {
		return dan;
	}

	public void setDan(int dan) {
		this.dan = dan;
	}

}
