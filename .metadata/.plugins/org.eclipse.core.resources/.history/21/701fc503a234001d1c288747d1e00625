package shape;

public class Circle extends Shape{
	
	protected double radius;
	
	public Circle() {
		super();
	}
	public Circle(double radius) {
		this.radius = radius;
	}

	@Override
	public void area() {
		double area;
		area = radius*radius*3.14;
		System.out.println(area);
	}

	public double getRadius() {
		return radius;
	}

	public void setRadius(double radius) {
		this.radius = radius;
	}
}
